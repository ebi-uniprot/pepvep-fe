{
  "name": "react-seed",
  "version": "1.0.0",
  "main": "./src/ui/index.jsx",
  "repository": "git@github.com:ebi-uniprot/react-seed.git",
  "author": "Mahdi Mahmoudy <mmahmoudy@ebi.ac.uk>",
  "license": "MIT",
  "scripts": {
    "local-server": "webpack --config webpack.local.js --mode development && webpack-dev-server --config webpack.local.js --mode development --open",
    "dev-build": "webpack --config webpack.dev.js --mode development",
    "build": "webpack --mode production",
    "jslint": "./node_modules/.bin/eslint -c .eslintrc.json --ext .js,.jsx 'src/**' '__tests__/**' '__mocks__/**' --ignore-pattern 'src/styles/**'",
    "test": "jest",
    "update-snapshots": "jest --updateSnapshot",
    "build-css": "node-sass-chokidar src/styles -o src/styles"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testURL": "http://localhost/",
    "globals": {
      "BASE_URL": "/uniprot/pepvep",
      "API_URL": "/uniprot/pepvep"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "litemol": "^2.4.2",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.1",
    "protvista-structure": "^2.3.5",
    "react": "^16.3.2",
    "react-dom": "16.3.2",
    "react-router-dom": "^4.2.2",
    "uuid": "^3.3.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "css-loader": "^0.28.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.3",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.0"
  }
}
